<div class="my-subscriptions">
  <div class="container">
    <div class="row">
      <div class="col-md-12 box-white">
        <table class="table table-hover">
          <thead>
            <tr>
              <th>Service</th>
              <th>Category Service</th>
              <th>Credit</th>
              <th>Credit Paid from Company</th>
              <th>Credit Paid from you</th>
              <th></th>
            </tr>
          </thead>
          <tbody>
            <% @item_subscriptions.each do |item_subscription| %>
              <tr>
                <th><%= image_tag item_subscription.item.image.url(:thumb) %></th>
                <th><%= item_subscription.item.item_category.name %></th>
                <th><%= item_subscription.amount_credit %></th>
                <th>To do</th>
                <th>To do</th>
                <th>
                  <%= link_to "Edit", edit_item_subscription_path(item_subscription.id), class: 'btn btn-primary' %>
                  <%= link_to "Remove", item_subscription_path(item_subscription.id), method: :delete, class: 'btn btn-danger' %>
                </th>
              </tr>
            <% end %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>
